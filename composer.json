{
    "name": "hocza/sendy",
    "description": "Sendy API implementation for Laravel",
    "keywords": ["laravel", "sendy", "email", "newsletter", "API"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jozsef Hocza",
            "email": "jozsef@hocza.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Hocza\\Sendy\\": "src/",
            "Hocza\\Tests\\Sendy\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hocza\\Sendy\\SendyServiceProvider"
            ],
            "aliases": {
                "Sendy": "Hocza\\Sendy\\Facades\\Sendy"
            }
        }
    }
}
